{
  "expo": {
    "name": "List Easy",
    "slug": "listeasy",
    "version": "3.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "userInterfaceStyle": "automatic",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#fff"
      },
      "package": "com.penpack.listeasy",
      "versionCode": 20,
      "permissions": [
        "com.android.vending.BILLING"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "021d95ea-9341-4a16-bed5-85eeacc48547"
      }
    },
    "plugins": [
      "sentry-expo",
      ["react-native-fbsdk-next",
        {
          "appID": "3387831338148065",
          "clientToken": "0b7d5fa12d9e71aaf35ecf28b1c8773e",
          "displayName": "List Easy",
          "scheme": "fb3387831338148065",
          "advertiserIDCollectionEnabled": true,
          "autoLogAppEventsEnabled": true,
          "isAutoInitEnabled": true
        }]      
    ]
  },
  "react-native-google-mobile-ads": {
    "android_app_id": "ca-app-pub-8430347978354434~3537975748"
  },
  "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "penpack",
            "project": "list-easy"
          }
        }
      ]
    }
}
